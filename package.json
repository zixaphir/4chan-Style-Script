{
  "name": "4chanSS",
  "version": "3.1.0",
  "description": "Customize 4chan to your liking right on the page itself.",
  "meta": {
    "name": "4chan Style Script",
    "namespace": "ahodesuka.github.com",
    "repo": "https://github.com/zixaphir/4chan-Style-Script/",
    "page": "https://github.com/zixaphir/4chan-Style-Script/",
    "buildsPath": "builds/",
    "mainBranch": "master",
    "matches": [
      "*://boards.4chan.org/*",
      "*://sys.4chan.org/*",
      "*://a.4cdn.org/*",
      "*://i.4cdn.org/*"
    ],
    "files": {
      "metajs": "4chanSS.meta.js",
      "userjs": "4chanSS.user.js"
    },
    "min": {
      "chrome": "29",
      "firefox": "22",
      "greasemonkey": "1.12"
    }
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-bump": "~0.0.11",
    "grunt-concurrent": "~0.4.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-contrib-concat": "~0.3.0",
    "load-grunt-tasks": "~0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zixaphir/4chan-Style-Script.git"
  },
  "author": "Ahodesuka",
  "contributors": [
    "Zixaphir",
    "Nicolas Stepien <stepien.nicolas@gmail.com>",
    "James Campos <james.r.campos@gmail.com>"
  ],
  "license": "",
  "readmeFilename": "",
  "engines": {
    "node": ">=0.10"
  }
}
